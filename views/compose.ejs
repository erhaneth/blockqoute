<h1>Read Post</h1>
      <p><%= quote.quote %></p>
      <h4><%= quote.author %></h4>
      <p><%= quote.body %></p>
<form class="" action="/compose" method="post">
  <div class="">
    <input type="hidden" value='"<%= quote.content  %>" <%= quote.author  %>' name="quote">
    <p>
      <%= quote.content %>
        <%= quote.author %>
          <%= quote.composeId %>
    </p>
    <label>Write your thoughts:</label>
    <div>
      <textarea class="" name="postBody" rows="8" cols="100"></textarea>
    </div>
  </div>
  <button class="" type="submit" name="button">Publish</button>
</form>
<button class="" name="button"><a href="/compose/<%= quote.id%>/edit">Edit</a></button>